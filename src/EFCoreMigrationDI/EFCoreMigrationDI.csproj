<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>net6.0</TargetFramework>
		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>enable</Nullable>
		<WarningsAsErrors>Nullable</WarningsAsErrors>
		<RootNamespace>Dasein.EntityFrameworkCore.Migrations</RootNamespace>
		<AnalysisLevel>latest-all</AnalysisLevel>
		
		<GeneratePackageOnBuild>True</GeneratePackageOnBuild>
		<AssemblyVersion>6.0.10.0</AssemblyVersion>
		<FileVersion>6.0.10.1</FileVersion>
		<VersionPrefix>6.0.10</VersionPrefix>
		<VersionSuffix>1</VersionSuffix>
		<Title>EF Core Migration Dependency Injection</Title>
		<Company />
		<Product />
		<PackageLicenseExpression>MIT</PackageLicenseExpression>
		<PackageId>Dasein.MigrationDI</PackageId>
		<Description>Set of extension methods providing DI to EF Core for SQL Server</Description>
		<PackageTags> Entity;Framework;Core;entity-framework-core;EF;Data;O/RM;EntityFrameworkCore;EFCore;SQL;Server;DI;Dependency;Injection</PackageTags>
		<NeutralLanguage>en-US</NeutralLanguage>
		
	</PropertyGroup>

	<ItemGroup>
		<None Include="..\..\LICENSE.md" Pack="true" PackagePath="LICENSE.md" />
	</ItemGroup>

	<ItemGroup>
		<PackageReference Include="Microsoft.EntityFrameworkCore.Relational" Version="[6.0.0,6.1)" />
	</ItemGroup>

</Project>
